<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ListView
        android:id="@+id/kt_common_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/kryptanium_list" />

    <TextView
        android:id="@+id/kt_common_click_button"
        android:layout_gravity="center_horizontal"
        android:text="@string/kt_report"
        style="@style/kryptanium_button_style_main"
        android:layout_marginTop="@dimen/kt_content_padding" />


</LinearLayout>
